{
  "manifest_version": 3,
  "name": "Tab Share",
  "version": "1.0.0",
  "description": "Chrome extension that enables you to share you tabs with anyone",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "tabs",
    "clipboardRead",
    "clipboardWrite"
  ],
  "host_permissions": [
    "https://ataman.cloud/*"
  ],
  "action": {
    "default_popup": "index.html"
  }
}
